<div class="row mt-5">
        <div class="col-md-6 m-auto">
          <div class="card card-body">
            <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i> Inicia Sesion</h1>
            <% include ./partials/messages %>
            <form action="/users/login" method="POST">
              <div class="form-group">
                <label for="email">Correo Electronico</label>
                <input
                  type="email"
                  id="email"
                  name="email"
                  class="form-control"
                  placeholder="Correo Electronico"
                />
              </div>
              <div class="form-group">
                <label for="password">Clave</label>
                <input
                  type="password"
                  id="password"
                  name="password"
                  class="form-control"
                  placeholder="Clave"
                />
              </div>
              <button type="submit" class="btn btn-primary btn-block">Iniciar Sesion</button>
            </form>
            <p class="lead mt-4">
              No tienes cuenta? <a href="/users/register">Registrarse</a>
            </p>
          </div>
        </div>
      </div>